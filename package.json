{
  "name": "af-backup",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "basic-ftp": "^5.0.5",
    "colors": "^1.4.0",
    "dotenv": "^16.4.5",
    "joi": "^17.13.3",
    "minio": "^8.0.1",
    "nodemon": "^3.1.4",
    "pkg": "^5.8.1",
    "tar": "^7.4.3",
    "unbzip2-stream": "^1.4.3"
  },
  "scripts": {
    "start": "nodemon index.js",
    "build:backup": "pkg index.js --target node18-linux-x64 --output dist/af-backup",
    "build:restore": "pkg restore.js --target node18-linux-x64 --output dist/af-restore",
    "build": "yarn build:backup && yarn build:restore",
    "push": "node ./libs/push.js",
    "up_patch": "npm version patch",
    "up_minor": "npm version minor",
    "up_major": "npm version major",
    "commit": "git add . && git commit -m 'update v $(node -p \"require('./package.json').version\")'"
  },
  "version": "v2.1.4"
}